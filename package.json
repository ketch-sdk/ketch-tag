{
  "name": "@ketch-sdk/ketch-tag",
  "version": "0.0.1",
  "description": "Ketch TypeScript/JavaScript Web Tag",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "all": "npm run lint && npm run format-check && npm run test && npm run docs && npm run build",
    "build": "npm run build:lib && npm run build:web",
    "build:lib": "ncc build --minify --license licenses.txt src/index.ts",
    "build:web": "webpack --config webpack.config.js --mode production",
    "start": "npx webpack serve --devtool eval-source-map --mode development",
    "lint": "eslint src/**/*.ts",
    "test": "jest --runInBand --passWithNoTests",
    "format": "prettier --write \"**/*.{ts,tsx,yml,yaml}\"",
    "format-check": "prettier --check '**/*.ts'",
    "docs": "typedoc --githubPages true --excludeExternals --excludeInternal src/index.ts"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not dead"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ketch-sdk/ketch-tag.git"
  },
  "author": "Ketch (https://www.ketch.com/)",
  "license": "MIT",
  "homepage": "https://github.com/ketch-sdk/ketch-tag",
  "bugs": {
    "url": "https://github.com/ketch-sdk/ketch-tag/issues"
  },
  "devDependencies": {
    "@ketch-com/future": "^1.0.4",
    "@ketch-sdk/ketch-data-layer": "^1.1.1",
    "@ketch-sdk/ketch-web-api": "^1.0.10",
    "@types/fetch-mock": "^7.3.5",
    "@types/jest": "^29.2.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-prettier": "^4.2.1",
    "events": "^3.3.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^15.0.0",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3",
    "uuid": "^9.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@ketch-sdk/ketch-types": "^1.3.3",
    "process": "^0.11.10",
    "util": "^0.12.5"
  },
  "files": [
    "./dist/index.js",
    "./dist/index.d.ts"
  ]
}
